{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "requireConfigFile": false
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "vue/multi-word-component-names": "off"
  }
}
